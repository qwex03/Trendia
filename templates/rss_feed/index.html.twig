{% extends 'base.html.twig' %}

{% block title %}RssFeed index{% endblock %}

{% block body %}
    <h1>RssFeed index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>Name</th>
                <th>Url</th>
                <th>CreatedAt</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for rss_feed in rss_feeds %}
            <tr>
                <td>{{ rss_feed.id }}</td>
                <td>{{ rss_feed.name }}</td>
                <td>{{ rss_feed.url }}</td>
                <td>{{ rss_feed.createdAt ? rss_feed.createdAt|date('Y-m-d H:i:s') : '' }}</td>
                <td>
                    <a href="{{ path('app_rss_feed_show', {'id': rss_feed.id}) }}">show</a>
                    <a href="{{ path('app_rss_feed_edit', {'id': rss_feed.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="5">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('app_rss_feed_new') }}">Create new</a>
{% endblock %}
